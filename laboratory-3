static void Main(string[] args)
        {
            string x, y;
            int x1, y1;
            Console.WriteLine("Введите х ");
            x = Console.ReadLine();
            x1 = Convert.ToInt32(x);
            Console.WriteLine("Введите у ");
            y = Console.ReadLine();
            y1 = Convert.ToInt32(y);

            if (x1>0 && y1>0)
            {
                Console.WriteLine("Точка лежит в первом квадрате ");
            }
            else if (x1<0 && y1<0)
            {
                Console.WriteLine("Точка лежит в третем квадрате ");
            }
            else
            {
                Console.WriteLine("Точка лежит во втором или четвертом квадрате ");
            }

            Console.ReadKey();
        }
